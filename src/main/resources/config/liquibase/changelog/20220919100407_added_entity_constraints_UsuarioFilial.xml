<?xml version="1.0" encoding="utf-8"?>
<databaseChangeLog
    xmlns="http://www.liquibase.org/xml/ns/dbchangelog"
    xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
    xsi:schemaLocation="http://www.liquibase.org/xml/ns/dbchangelog http://www.liquibase.org/xml/ns/dbchangelog/dbchangelog-latest.xsd">
    <!--
        Added the constraints for entity UsuarioFilial.
    -->
    <changeSet id="20220919100407-2" author="jhipster">

        <addForeignKeyConstraint baseColumnNames="licenca_id"
                                 baseTableName="usuario_filial"
                                 constraintName="fk_usuario_filial__licenca_id"
                                 referencedColumnNames="id"
                                 referencedTableName="licenca"/>

        <addForeignKeyConstraint baseColumnNames="usuario_id"
                                 baseTableName="usuario_filial"
                                 constraintName="fk_usuario_filial__usuario_id"
                                 referencedColumnNames="id"
                                 referencedTableName="usuario"/>

        <addForeignKeyConstraint baseColumnNames="filial_id"
                                 baseTableName="usuario_filial"
                                 constraintName="fk_usuario_filial__filial_id"
                                 referencedColumnNames="id"
                                 referencedTableName="filial"/>
    </changeSet>
</databaseChangeLog>
