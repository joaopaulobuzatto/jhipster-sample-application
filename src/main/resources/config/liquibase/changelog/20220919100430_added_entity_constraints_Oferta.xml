<?xml version="1.0" encoding="utf-8"?>
<databaseChangeLog
    xmlns="http://www.liquibase.org/xml/ns/dbchangelog"
    xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
    xsi:schemaLocation="http://www.liquibase.org/xml/ns/dbchangelog http://www.liquibase.org/xml/ns/dbchangelog/dbchangelog-latest.xsd">
    <!--
        Added the constraints for entity Oferta.
    -->
    <changeSet id="20220919100430-2" author="jhipster">

        <addForeignKeyConstraint baseColumnNames="usuario_criador_id"
                                 baseTableName="oferta"
                                 constraintName="fk_oferta__usuario_criador_id"
                                 referencedColumnNames="id"
                                 referencedTableName="usuario"/>

        <addForeignKeyConstraint baseColumnNames="plano_id"
                                 baseTableName="oferta"
                                 constraintName="fk_oferta__plano_id"
                                 referencedColumnNames="id"
                                 referencedTableName="plano"/>
    </changeSet>
</databaseChangeLog>
