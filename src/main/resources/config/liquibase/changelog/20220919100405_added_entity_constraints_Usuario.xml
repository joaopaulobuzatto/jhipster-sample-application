<?xml version="1.0" encoding="utf-8"?>
<databaseChangeLog
    xmlns="http://www.liquibase.org/xml/ns/dbchangelog"
    xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
    xsi:schemaLocation="http://www.liquibase.org/xml/ns/dbchangelog http://www.liquibase.org/xml/ns/dbchangelog/dbchangelog-latest.xsd">
    <!--
        Added the constraints for entity Usuario.
    -->
    <changeSet id="20220919100405-2" author="jhipster">

        <addForeignKeyConstraint baseColumnNames="licenca_id"
                                 baseTableName="usuario"
                                 constraintName="fk_usuario__licenca_id"
                                 referencedColumnNames="id"
                                 referencedTableName="licenca"/>

        <addForeignKeyConstraint baseColumnNames="supervisor_id"
                                 baseTableName="usuario"
                                 constraintName="fk_usuario__supervisor_id"
                                 referencedColumnNames="id"
                                 referencedTableName="usuario"/>

        <addForeignKeyConstraint baseColumnNames="usuario_criador_id"
                                 baseTableName="usuario"
                                 constraintName="fk_usuario__usuario_criador_id"
                                 referencedColumnNames="id"
                                 referencedTableName="usuario"/>

        <addForeignKeyConstraint baseColumnNames="pessoa_fisica_id"
                                 baseTableName="usuario"
                                 constraintName="fk_usuario__pessoa_fisica_id"
                                 referencedColumnNames="id"
                                 referencedTableName="pessoa_fisica"/>

        <addForeignKeyConstraint baseColumnNames="horario_trabalho_id"
                                 baseTableName="usuario"
                                 constraintName="fk_usuario__horario_trabalho_id"
                                 referencedColumnNames="id"
                                 referencedTableName="horario_trabalho"/>
    </changeSet>
</databaseChangeLog>
